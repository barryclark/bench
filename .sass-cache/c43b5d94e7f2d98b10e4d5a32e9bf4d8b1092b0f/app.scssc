:@template"∑L@import "compass/css3";
@import "compass/reset";

/* =====================
variables
======================*/
$bg:			#131214;
$dropHeight: 	45px;
$logo:          'data:image/png;base64,';
$picWidth:      400px;
$picWidthSm:    $picWidth * .75;
$picMargin:     $picWidth / 20;
$wrapWidth:     ($picWidth * 3) + ($picMargin * 2);
$blue:			#3e97f2;
$pink:          #ec1350;


/*----------------------
Partials
----------------------*/
@import "partials/animation";
@import "partials/animation/animate";
@import "partials/flipper";

/*----------------------
Keyframes
----------------------*/
@include keyframes(avatar) {
	0% {
		@include transform(translateY(-20px) rotate(60deg));
		opacity: 0;
	}
	100% {
		@include transform(translateY(0px) rotate(0));
		opacity: 1;
	}
}

@include keyframes(fadeRight) {
	0% {
		@include transform(translateX(20px));
		opacity: 0;
	}
	100% {
		@include transform(translateX(0px));
		opacity: 1;
	}
}

/* =====================
Mixins
======================*/

@mixin btn($baseColor, $textColor, $fontSize) {
	cursor: pointer;
	display:inline-block;
	padding: ($fontSize / .9) $fontSize + .5;
	margin-top: 2em;
	text-transform:uppercase;
	width: 85%;
	text-align: center;
	font-size: $fontSize;
	border:1px solid darken($baseColor, 15%);
	background:$baseColor;
	@include background(linear-gradient(top, lighten($baseColor, 5%), $baseColor));
	@include border-radius(3px);
	@include box-shadow(inset 0 0px 2px lighten($baseColor, 25%));
	@include text-shadow(0 1px 0 darken($baseColor, 25%));
	@include transition(all .05s ease-in-out);
	color:$textColor;
	&:hover {
		@include background(linear-gradient(top, lighten($baseColor, 10%), $baseColor));
		@include box-shadow(inset 0 1px 0 lighten($baseColor, 5%));
	}
	&:active { // Adds Styles When Button Is Pushed
			@include background(linear-gradient(top, darken($baseColor, 5%), $baseColor));
			@include box-shadow(inset 0 0px 2px darken($baseColor, 35%));
			border-color: darken($baseColor, 18%);
			color: darken($baseColor, 25%);
			@include text-shadow(0 1px 0 lighten($baseColor, 5%));
	}
}

/* =====================
app styles
======================*/

@font-face {
	font-family: 'icomoon';
	src:url('fonts/icomoon.eot');
	src:url('fonts/icomoon.eot?#iefix') format('embedded-opentype'),
		url('fonts/icomoon.svg#icomoon') format('svg'),
		url('fonts/icomoon.woff') format('woff'),
		url('fonts/icomoon.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

/* Use the following CSS code if you want to use data attributes for inserting your icons */
[data-icon]:before {
	font-family: 'icomoon';
	content: attr(data-icon);
	speak: none;
	font-weight: normal;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
}

/* Use the following CSS code if you want to have a class per icon */
[class^="icon-"]:before, [class*=" icon-"]:before {
	font-family: 'icomoon';
	font-style: normal;
	speak: none;
	font-weight: normal;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
}
.icon-cc-by:before {
	content: "\21";
}
.icon-heart:before {
	content: "\22";
}
.icon-star:before {
	content: "\23";
}
.icon-users:before {
	content: "\24";
}
.icon-question-mark:before {
	content: "\25";
}

body {
	background-color:$bg;
	font-family: 'Open Sans', sans-serif;
	letter-spacing:-.01em;
	background-image:url(img/bg-noLogo.png), url(img/bgPat.png);
	//background:url(img/bgPat.png);
	background-position:center -10px, 0 0;
	background-repeat:no-repeat, repeat;
	text-align: center;
}

a {
	color:$blue;
	text-decoration:none;
	outline:none;
	&:hover {
		color:#ce4d73;
	}
}

.wrap {
	//width:$wrapWidth;
	width: 100%;
	margin:auto;
}

.head {
	padding: 10px 0;
	@include background-image(linear-gradient(rgba(0, 0, 0, .7), rgba(0, 0, 0, .9)));
	border-bottom:1px solid rgba(0, 0, 0, .9);
	@include box-shadow(inset 0 -1px 0 rgba(255, 255, 255, .2));
	position: fixed;
	overflow:hidden;
	width: 100%;
	top: 0;
	left: 0;
	right: 0;
	z-index: 100;
	@include animation(fadeInDown);
		-webkit-animation-duration:1s;
		-webkit-animation-fill-mode:both;
	.logo {
		background:url(img/logo.png) no-repeat;
		width:122px;
		height:20px;
		margin:3px 10px;
		text-indent:-9999px;
		float:left;
		background-size:100%;
	}
	.user {
		float: left;
		input {
			background: transparent;
			border: none;
			outline: none;
			color: $pink;
			font-size: .8em;
			padding: .3em;
		}
		
	}
	ul.nav {
		float: right;
		padding-right: 3em;
		position: relative;
		li {
			display: inline-block;
			padding: .3em .15em;
			color: #fff;
			font-size: .9em;
			font-weight: 600;
			cursor: pointer;
			a {
				color: white;
				font-size: 1.5em;
				cursor: pointer;
			}
			&.active {
				color: $pink;
			}
			&.settings {
				-webkit-mask: url('img/settings.png') no-repeat center center;
				 -webkit-mask-size: 12px 12px;
				position: absolute;
				right: 17px;
				top: 3px;
				width: 12px;
				height: 12px;
				background-size: 100%;
				padding: .1em;
				margin-left: .5em;
				background-color: #ccc;
				&:hover, &.active {
					background-color: $pink;
				}
			}
		}
	}

	.loggedIn {
		float:right;
		width:40%;
		@include animation(fadeInRight);
		-webkit-animation-duration:1s;
		-webkit-animation-fill-mode:both;
		.avatar {
			height:30px;
			border-radius:3px;
			float:right;
			@include box-shadow(inset 0 1px 0 #fff);
			margin-left:5px;
		}
		.name {
			text-align:right;
			color:#fff;
			font-weight:700;
			font-size:80%;
			white-space: nowrap;
			overflow: hidden;
			text-overflow: ellipsis;
			width: 120px;
			@media screen and (max-width: 1300px) {
			    width: 100px;
			    //height:$picWidthSm / 4 * 3;
			}
		}
		.stats {
			text-align:right;
			color:#ccc;
			font-size:70%;
		}
	}
}

.pics {
	//overflow:hidden;
	margin-top: 50px;
	#picsList {
		@include animation(fadeInUp 1s ease-in);
		margin: auto;
		width: 100%;
		> div {
			-webkit-transform: rotate3d(0,0,0,0);
			width:$picWidth;
			@media screen and (max-width: 1300px) {
			    width: $picWidthSm;
			    //height:$picWidthSm / 4 * 3;
			}
			margin: 8px;
			display: inline-block;
			position:relative;
			background: url('img/ajax-loader.gif') center center no-repeat;
			&.animate {
				-webkit-animation-duration:1s;
				-webkit-animation-fill-mode:both;
			}
			&.fadeInUp{   
				@include animation-name(fadeInUp)
			}
			&.bounceIn {
				@include animation-name(bounceIn);
			}
			/*&:nth-child(3n+3) {
				padding-right:0;
			}*/
			&.featured {
				span.featured {
					position: absolute;
					top: -8px;
					right: -9px;
					background-image: url(img/featured.png);
					background-repeat: no-repeat;
					background-size: 100%;
					width: 63px;
					height: 62px;
					z-index: 100;
				}
			}
			.picture {
				position:relative;
				img {
					@include box-shadow(inset 0 1px 0 #fff);
				}
			}
			
		}
	}
}

.gradient {
	opacity: 0;
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	@include background-image(radial-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, .75)));
	@include transition(all .50s cubic-bezier(0.000, 0.910, 0.395, 1.3));
	a {
		position: absolute;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;
	}
	&:hover {
		opacity: 1;
	}
}

.footer {
	width:100%;
	overflow:hidden;
	@include animation(fadeInUp 1.5s ease-in);
	.more {
		border: 1px solid lighten($bg, 3%);
		font-weight:700;
		font-size: 12px;
		padding: 7px 10px;
		white-space: nowrap;
		vertical-align: middle;
		color: lighten($bg, 10%);
		background: transparent;
		cursor: pointer;
		border-radius:3px;
		border: 1px solid lighten($bg, 5%);
		@include box-shadow(0 1px 0 lighten($bg, 10%));
		//@include transition(all .15s ease-in-out);
		display: block;
		position:absolute;
		left:50%;
		margin-left:-30px;
		text-decoration:none;
		&:hover {
			border-color: #999;
			background: -webkit-linear-gradient(top, white, #E0E0E0);
			box-shadow: 0 1px 2px rgba(0, 0, 0, 0.5), inset 0 0 3px white;
			text-decoration: none;
			color: lighten($bg, 20%);
			text-shadow:0 1px 1px white;
		}
	}
	.settings {
		text-indent:-9999px;
		background:url(img/settings.png) no-repeat;
		width:28px;
		height:28px;
	  	display:block;
	  	margin:auto;
		opacity:.4;
		background-size:100%;
		@include transition(all .25s 4ms both ease-in-out);
		&:hover {
			opacity:.8;
		}
	}
}

/* =====================
Settings Styles
======================*/

.overlay {
	position:fixed;
	top:0;
	left:0;
	right:0;
	bottom:0;
	display:none;
	background: #000;
	opacity: .5;
}

.content {
	position:relative;
	width:$wrapWidth;
	margin:auto;
	margin-top:5%;
	z-index:100;
	text-align: left;
	input {
		border:0;
		background:transparent;
		font-size:2em;
		font-weight:200;
		letter-spacing:-.01em;
		color:$blue;
		font-family: 'Open Sans', sans-serif;
		padding-top:.5em;
		border-bottom:1px dashed #333;
		width:60%;
		&:focus {
			outline:none;
		}
		&.error {
			color:#ce4d73;
		}
		//&.playerList {
		//	color:#81ce4d;
		//}	
	}
	h2 {
		font-weight:200;
		font-size:3em;
		padding-top: 1.5em;
		color:#fff;
	}
	p {
		color:#ccc;
		padding-top:1em;
		line-height:1.4em;
		&.subInfo {
			font-size:.8em;
			color:#999;
		}
	}
	.close {
		text-indent:-9999px;
		background:url(img/close.png) no-repeat;
		width:28px;
		height:28px;
	  	display:block;
	  	margin:auto;
		opacity:.4;
		background-size:100%;
		margin-top:70px;
		cursor:pointer;
		@include transition(all .25s 4ms both ease-in-out);
		&:hover {
			opacity:.6;
		}
	}
}
.overlayBg {
	position:absolute;
	top:0;
	left:0;
	right:0;
	bottom:0;
	background:#000;
	opacity:.95;
	z-index:50;
}

/*------------------------
Setting Styles
-------------------------*/
.settingsPop {
	position: fixed;
	top: 60px;
	right: 10px;
	display: none;
	@include background-image(linear-gradient(lighten(#000, 7%), lighten(#000, 3%)));
	border:1px solid #000;
	padding: 20px;
	border-radius: 3px;
	//box-shadow: 0 1px 3px black;
	z-index: 3000;
	&.in {
		@include animation(fadeInUp 700ms both cubic-bezier(.5, .5, .45, 2));
	}
	&.out {
		@include animation(fadeOutDown .5s 0ms both cubic-bezier(0, 0, .45, 1));
	}
	&:after, &:before {
		bottom: 100%;
		border: solid transparent;
		content: " ";
		height: 0;
		width: 0;
		position: absolute;
		pointer-events: none;
	}

	&:after {
		border-color: rgba(0, 0, 0, 0);
		border-bottom-color: lighten(#000, 7%);
		border-width: 6px;
		right: 6px;
		margin-left: -6px;
	}
	&:before {
		border-color: rgba(0, 0, 0, 0);
		border-bottom-color: #000;
		border-width: 7px;
		right: 7px;
		margin-left: -7px;
	}
}

a.donate {
	@include btn($blue, #fff, .8em);
}

.content {
	position:relative;
	width:240px;
	margin:auto;
	z-index:100;
	&.in {
		-webkit-animation: fadeInDown .08s 150ms both cubic-bezier(0, 0, .45, 1);
	}
	&.out {
		-webkit-animation: fadeOutUp .08s 150ms both cubic-bezier(0, 0, .45, 1);
	}
	input[type="text"] {
		border:0;
		background:#000;
		font-size:1em;
		font-weight:200;
		letter-spacing:-.01em;
		color:$pink;
		font-family: 'Open Sans', sans-serif;
		padding:.5em;
		width: 14em;
		border-bottom: 1px solid #222;
		border-radius: 2px;
		&:focus {
			outline:none;
		}
		&.error {
			color:#ce4d73;
		}
	}
	h2 {
		font-weight:200;
		font-size:1.2em;
		padding-top:1.5em;
		margin-bottom: .5em;
		color:#ccc;
	}
	p {
		color:#ccc;
		padding-top:.4em;
		line-height:1.4em;
		font-size: .8em;
		&.subInfo {
			font-size:.8em;
			color:#999;
		}
	}
	.close {
		text-indent:-9999px;
		background:url(img/close.png) no-repeat;
		width:28px;
		height:28px;
	  	display:block;
	  	margin:auto;
		opacity:.4;
		background-size:100%;
		margin-top:70px;
		cursor:pointer;
		@include transition(all .25s 4ms both ease-in-out);
		&:hover {
			opacity:.6;
		}
	}
}

.blur {
	-webkit-filter: blur(3px);
}

.clear {
	clear:both;
}
